@model Adminodash.Entities.Project
@{
    ViewBag.Title = "ProjectAdd";
    Layout = "~/Views/AdminLayout/Dashboard.cshtml";
}

@using (Html.BeginForm("ProjectAdd", "Project", FormMethod.Post))
{
<div class="form-control">
    @Html.LabelFor(x => x.ProjectName, new { @class = "form-control" })
    @Html.TextBoxFor(x => x.ProjectName, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.ProjectName)
    <br />
    @Html.LabelFor(x => x.ProjectDescription, new { @class = "form-control" })
    @Html.TextBoxFor(x => x.ProjectDescription, new { @class = "form-control" })
    <br />
    <label class="form-control">Proje Kategorisi</label>
    @Html.DropDownListFor(x => x.ProjectCategoryId, (List<SelectListItem>)ViewBag.category, new { @class = "form-control" })
    <br />
    <label class="form-control">Proje Kategorisi</label>
    @Html.DropDownListFor(x => x.ProjectStatusId, (List<SelectListItem>)ViewBag.status, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.ProjectProgress, new { @class = "form-control" })
    @Html.TextBoxFor(x => x.ProjectProgress, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.ProjectProgress)
    <br />
    <button class="btn btn-info">Yeni Sayfa Verisi Ekle</button>
</div>
}

